    <script>
        $(function () {
            $('#btnAncillaryCost').click(function () {
                $(this).attr("data-toggle", "modal").attr("data-target", "#exampleModal");
            });
            $('#btnRecuringCost').click(function () {
                $(this).attr("data-toggle", "modal").attr("data-target", "#RecuriingCost");
            });

